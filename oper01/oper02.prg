
/*
Tipo de dado e operador
*/
PROCEDURE Main
LOCAL x, y, z 

   x := 2
   y := 3
   z := 4
   
   ? "Precedência de operadores matemáticos"
   ?
   ? "Dados x = ", x
   ? "      y = ", y
   ? "      z = ", z
   ?
   ? "Exemplos"
   ?
   ?  "x + y * z = " , x + y * z 
   ?  "x / y ^ 2 = " , x / y ^ 2
   ?  " -x ^ 3   = " , -x ^ 3
              
RETURN


